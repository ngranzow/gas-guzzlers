<div class="cars">
    <div class="car">
        <a href="/dashboard/car/{{id}}" class="car-link">{{Make}} {{Model}} {{Year}}</a>
    </div>
    <div class="commute">
        <a href="/dashboard/car/{{id}}" class="commute-link">This car has {{commutes.length}} {{format_plural "commute" commutes.length}}</a>
    </div>
    <div class="gas">
        <a href="/dashboard/car/{{id}}" class="gas-link">This car has {{gas.length}} gas {{format_plural "price" gas.length}}</a>
    </div>
</div>